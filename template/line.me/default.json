{
    "messages": [
        {
            "type": "flex",
            "altText": "A message sent by https://github.com/up9cloud/action-notify",
            "contents": {
                "type": "bubble",
                "body": {
                    "type": "box",
                    "layout": "vertical",
                    "contents": [
                        {
                            "type": "box",
                            "layout": "baseline",
                            "contents": [
                                {
                                    "type": "text",
                                    "text": "${GITHUB_JOB_STATUS_EMOJI} GitHub action #${GITHUB_RUN_NUMBER}: ${GITHUB_JOB_STATUS}",
                                    "size": "md",
                                    "action": {
                                        "type": "uri",
                                        "label": "action",
                                        "uri": "https://github.com/${GITHUB_REPOSITORY}/actions/runs/${GITHUB_RUN_ID}"
                                    },
                                    "wrap": true
                                }
                            ]
                        },
                        {
                            "type": "box",
                            "layout": "baseline",
                            "contents": [
                                {
                                    "type": "text",
                                    "text": "${GITHUB_REPOSITORY}",
                                    "size": "sm",
                                    "color": "#999999",
                                    "action": {
                                        "type": "uri",
                                        "label": "action",
                                        "uri": "https://github.com/${GITHUB_REPOSITORY}"
                                    },
                                    "wrap": true,
                                    "align": "end"
                                }
                            ]
                        },
                        {
                          "type": "separator",
                          "color": "#eeeeee",
                          "margin": "sm"
                        },
                        {
                            "type": "box",
                            "layout": "baseline",
                            "contents": [
                                {
                                    "type": "text",
                                    "text": "${GIT_COMMIT_MESSAGE_ESCAPED}",
                                    "size": "sm",
                                    "color": "#999999",
                                    "wrap": true,
                                    "maxLines": 3
                                }
                            ],
                            "margin": "sm"
                        },
                        {
                            "type": "box",
                            "layout": "baseline",
                            "contents": [
                                {
                                    "type": "text",
                                    "text": "commit ${GITHUB_SHA_SHORT}",
                                    "size": "sm",
                                    "color": "#999999",
                                    "action": {
                                        "type": "uri",
                                        "label": "action",
                                        "uri": "https://github.com/${GITHUB_REPOSITORY}/commit/${GITHUB_SHA}"
                                    },
                                    "wrap": true,
                                    "align": "end"
                                }
                            ]
                        },
                        {
                          "type": "separator",
                          "color": "#eeeeee",
                          "margin": "sm"
                        },
                        {
                            "type": "box",
                            "layout": "baseline",
                            "contents": [
                                {
                                    "type": "text",
                                    "text": "by ${GIT_COMMITTER_USERNAME}",
                                    "size": "sm",
                                    "color": "#999999",
                                    "action": {
                                        "type": "uri",
                                        "label": "action",
                                        "uri": "https://github.com/${GIT_COMMITTER_USERNAME}"
                                    },
                                    "wrap": true,
                                    "align": "end"
                                },
                                {
                                    "type": "icon",
                                    "url": "https://avatars.githubusercontent.com/${GIT_COMMITTER_USERNAME}",
                                    "size": "xl",
                                    "margin": "sm",
                                    "offsetTop": "sm"
                                }
                            ]
                        }
                    ]
                }
            }
        }
    ]
}